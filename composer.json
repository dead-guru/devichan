{
  "name": "dead-guru/devichan",
  "description": "devichan imageboard",
  "type": "project",
  "require": {
    "php": ">=8.4",
    "ext-mbstring": ">=8.4",
    "ext-gd": ">=8.4",
    "ext-curl": ">=8.4",
    "ext-pdo": ">=8.4",
    "ext-fileinfo": ">=8.4",
    "twig/twig": "^3",
    "lifo/ip": "^1.1",
    "gettext/gettext": "^1.0",
    "geoip/geoip": "^1.17",
    "dapphp/securimage": "dev-nextgen",
    "ext-gettext": "*",
    "ext-imagick": "*",
    "mrclay/jsmin-php": "^2.4",
    "matthiasmullie/minify": "^1.3",
    "ezyang/htmlpurifier": "^4.16",
    "bepsvpt/blurhash": "^2.1"
  },
  "autoload": {
    "classmap": [
      "inc/"
    ],
    "files": [
      "inc/bootstrap.php",
      "inc/display.php",
      "inc/template.php",
      "inc/database.php",
      "inc/events.php",
      "inc/api.php",
      "inc/mod/auth.php",
      "inc/lock.php",
      "inc/queue.php",
      "inc/polyfill.php",
      "inc/functions.php"
    ]
  },
  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/alex300/securimage.git"
      }
  ],
  "license": "Tinyboard + vichan",
  "authors": [
    {
      "name": "savetheinternet",
      "homepage": "https://github.com/savetheinternet"
    },
    {
      "name": "czaks",
      "homepage": "https://github.com/czaks"
    },
    {
      "name": "ctrlcctrlv",
      "homepage": "https://github.com/ctrlcctrlv"
    },
    {
      "name": "h00j",
      "homepage": "https://github.com/h00j"
    },
    {
      "name": "assada",
      "homepage": "https://github.com/assada"
    }
  ],
  "require-dev": {
    "symfony/var-dumper": "^6.2"
  }
}
